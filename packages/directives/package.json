{
  "name": "@graphql-reshape/directives",
  "description": "A collection of custom graphql directives",
  "version": "0.3.2",
  "author": {
    "name": "Kyle Scully",
    "url": "https://github.com/zieka"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/index.js",
  "keywords": [
    "graphql",
    "reshape",
    "directive",
    "directives",
    "@graphql-reshape",
    "@graphql-reshape/directives",
    "mod",
    "schema"
  ],
  "license": "MIT",
  "repository": "https://github.com/zieka/graphql-reshape/tree/master/packages/directives",
  "scripts": {
    "build": "tsc",
    "check-types": "tsc --noEmit",
    "prettier-check": "prettier --config package.json --list-different '{,**/}*.{js,ts,json,graphql}'",
    "prettier-format": "prettier --config package.json --write '{,**/}*.{js,ts,json,graphql}'"
  },
  "peerDependencies": {
    "graphql": "^14.1.1",
    "graphql-tools": "^4.0.4"
  },
  "prettier": {
    "printWidth": 120,
    "tabWidth": 2,
    "useTabs": false,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "none",
    "bracketSpacing": true,
    "jsxBracketSameLine": true,
    "arrowParens": "always"
  }
}
